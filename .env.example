# AGI Prompt Engineering Framework Configuration

# Server Configuration
PORT=3000
NODE_ENV=development
LOG_LEVEL=info

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_ORG_ID=your_org_id_here

# Anthropic Configuration (Optional)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Redis Configuration (Memory System)
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=
REDIS_DB=0

# Database Configuration (Optional - for persistent storage)
DATABASE_URL=postgresql://username:password@localhost:5432/agi_framework
DB_POOL_SIZE=10

# Vector Database Configuration (Optional)
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=your_pinecone_environment
PINECONE_INDEX_NAME=agi-knowledge-base

CHROMADB_URL=http://localhost:8000
WEAVIATE_URL=http://localhost:8080

# Monitoring and Analytics
PROMETHEUS_ENABLED=true
PROMETHEUS_PORT=9090

# Security Configuration
JWT_SECRET=your_jwt_secret_here
CORS_ORIGIN=http://localhost:3000,https://yourdomain.com
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Fine-tuning Configuration
FINE_TUNING_ENABLED=true
TRAINING_DATA_PATH=./data/training
MODEL_OUTPUT_PATH=./models
MAX_TRAINING_JOBS=5

# Multi-Agent Configuration
MAX_AGENTS_PER_TASK=10
AGENT_TIMEOUT_MS=30000
PARALLEL_EXECUTION_LIMIT=5

# Constitutional AI Configuration
STRICT_MODE_DEFAULT=false
CUSTOM_PRINCIPLES_PATH=./config/principles.json

# Performance Configuration
MAX_TOKENS_PER_REQUEST=4000
DEFAULT_TEMPERATURE=0.7
REQUEST_TIMEOUT_MS=60000
CACHE_TTL_SECONDS=3600

# Logging Configuration
LOG_FILE_PATH=./logs/agi-framework.log
LOG_MAX_FILE_SIZE=50MB
LOG_MAX_FILES=10
LOG_DATE_PATTERN=YYYY-MM-DD

# Development Configuration
ENABLE_PLAYGROUND=true
DEBUG_MODE=false
PROFILING_ENABLED=false

# Webhook Configuration (Optional)
WEBHOOK_SECRET=your_webhook_secret_here
WEBHOOK_ENDPOINTS=https://your-app.com/webhooks/agi

# Third-party Integrations
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token
DISCORD_BOT_TOKEN=your-discord-bot-token
TEAMS_WEBHOOK_URL=https://outlook.office.com/webhook/...

# Business Information
COMPANY_NAME=RJ Business Solutions
CONTACT_EMAIL=rjbizsolution23@gmail.com
WEBSITE_URL=https://rickjeffersonsolutions.com
SUPPORT_URL=https://rickjeffersonsolutions.com/contact