{
  "name": "agi-prompt-engineering",
  "version": "2.0.0",
  "description": "Advanced AGI Prompt Engineering Framework with Multi-Agent Orchestration",
  "main": "src/index.ts",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "docs:serve": "docsify serve docs",
    "docker:build": "docker build -t agi-prompt-framework .",
    "docker:run": "docker run -p 3000:3000 agi-prompt-framework"
  },
  "keywords": [
    "AGI",
    "prompt-engineering",
    "multi-agent",
    "reasoning",
    "chain-of-thought",
    "react",
    "llm",
    "fine-tuning",
    "rag",
    "constitutional-ai"
  ],
  "author": "Rick Jefferson <rjbizsolution23@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rjbizsolution23-wq/agi-prompt-engineering.git"
  },
  "dependencies": {
    "@langchain/core": "^0.1.5",
    "@langchain/openai": "^0.0.14",
    "@pinecone-database/pinecone": "^1.1.0",
    "anthropic": "^0.17.1",
    "axios": "^1.6.7",
    "chromadb": "^1.7.3",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "openai": "^4.26.0",
    "redis": "^4.6.12",
    "tiktoken": "^1.0.10",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.10",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
